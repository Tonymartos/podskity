{
  "name": "podskity",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "server-dev": "npx eslint . && cd ./webpack && webpack-dev-server --config webpack.config.dev.js",
    "prepare": "husky install",
    "lint": "npx eslint ."
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-react",
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-react-jsx"
    ]
  },
  "dependencies": {
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.8.2",
    "react-scripts": "5.0.1"
  },
  "devDependencies": {
    "@babel/plugin-syntax-jsx": "7.18.6",
    "@babel/preset-react": "7.18.6",
    "@babel/runtime": "^7.21.0",
    "@svgr/webpack": "6.5.1",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "13.5.0",
    "babel-core": "6.26.3",
    "babel-loader": "9.1.2",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "css-loader": "6.7.3",
    "eslint": "8.35.0",
    "eslint-plugin-react": "7.32.2",
    "html-react-parser": "^3.0.12",
    "html-webpack-plugin": "5.5.0",
    "husky": "8.0.3",
    "node-sass": "^7.0.3",
    "postcss-loader": "7.0.2",
    "sass": "1.58.3",
    "sass-loader": "13.2.0",
    "style-loader": "3.3.1",
    "url-loader": "4.1.1",
    "web-vitals": "2.1.4",
    "webpack": "5.75.0",
    "webpack-cli": "5.0.1",
    "webpack-dev-server": "4.11.1"
  }
}
